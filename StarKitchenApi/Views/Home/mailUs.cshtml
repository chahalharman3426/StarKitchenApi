
@{
    ViewBag.Title = "mailUs";
}
<div class="banner1">
    <div class="container">
        <h3 class="animated wow slideInLeft" data-wow-delay=".5s"><a href="index.html">Home</a> / <span>Mail Us</span></h3>
    </div>
</div>
<!--banner-->
<div class="content">
    <!--mail-->
    <div class="mail-w3">
        <div class="container">
            <h2 class="tittle">Mail Us</h2>
            <div class="mail-w3l">
                <div class="col-md-6 mail-grid">
                    <form action="#" method="post">
                        <input type="text" name="Name" value="Name" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Name';}" required="">
                        <input type="email" name="Email" value="Email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email';}" required="">
                        <input type="text" name="Phone" value="Mobile number" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Mobile number';}" required="">
                        <textarea type="text" name="Message" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Message...';}" required="">Message...</textarea>
                        <input type="submit" value="Send">
                    </form>
                </div>
                <div class="col-md-6 mail-grid">
                    <div class="newsletter">
                        <h4><span></span>Newsletter</h4>
                    </div>
                   
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="map">
                <iframe src="https://www.google.com/maps/embed?pb=!1m10!1m8!1m3!1d2482.432383990807!2d0.028213999961443994!3d51.52362882484525!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sin!4v1423469959819" frameborder="0" style="border:0"></iframe>
            </div>
        </div>
    </div>
    <!--mail-->
</div>
